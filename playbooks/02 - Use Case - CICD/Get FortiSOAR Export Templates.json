                "params": {
                    "python_function": "import json\nimport shutil\nimport pathlib\n\np = pathlib.Path('{{vars.steps.Unzip_Export_Template.data.code_output[0] | win_dirname }}\/records\/export_templates')\np.mkdir(parents=True, exist_ok=True)\n\nwith open('{{vars.export_template_one}}', 'w', encoding='utf-8') as f:\n    json.dump([{{vars.other_config_info[0]}}], f, ensure_ascii=False, indent=4)\n    \nwith open('{{vars.export_template_two}}', 'w', encoding='utf-8') as f:\n    json.dump([{{vars.other_config_info[1]}}], f, ensure_ascii=False, indent=4)\n\noldFilePath = pathlib.Path('\/tmp\/{{vars.steps.Get_Export_Job_Details.data.file.filename}}')\nif oldFilePath.is_file():\n     oldFilePath.unlink()\n\nif '{{vars.old_path}}':\n  \tshutil.move('{{vars.old_path}}', '{{vars.new_path}}')\nelse:\n    with open('{{vars.current_export_template}}', 'w', encoding='utf-8') as f:\n    \tjson.dump([{{vars.config_info[0]}}], f, ensure_ascii=False, indent=4)\n    \nshutil.make_archive('{{vars.zip_file}}', 'zip', '{{vars.zip_dir}}')"
                },
